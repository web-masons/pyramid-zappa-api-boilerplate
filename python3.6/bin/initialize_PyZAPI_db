#!/vagrant/python3.6/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'PyZAPI','console_scripts','initialize_PyZAPI_db'
__requires__ = 'PyZAPI'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('PyZAPI', 'console_scripts', 'initialize_PyZAPI_db')()
    )
